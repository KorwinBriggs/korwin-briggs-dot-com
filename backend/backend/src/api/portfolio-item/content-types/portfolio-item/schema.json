{
  "kind": "collectionType",
  "collectionName": "portfolio_items",
  "info": {
    "singularName": "portfolio-item",
    "pluralName": "portfolio-items",
    "displayName": "Portfolio Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mainPic": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "morePics": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "post": {
      "type": "richtext"
    },
    "seoBlurb": {
      "type": "text",
      "required": true
    },
    "portfolio_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::portfolio-tag.portfolio-tag",
      "inversedBy": "portfolio_items"
    },
    "slug": {
      "type": "string"
    }
  }
}
